<!doctype html>
<!--[if lt IE 7]><html class="no-js ie ie6 lt-ie9 lt-ie8 lt-ie7" {{site.language_attributes}}> <![endif]-->
<!--[if IE 7]><html class="no-js ie ie7 lt-ie9 lt-ie8" {{site.language_attributes}}> <![endif]-->
<!--[if IE 8]><html class="no-js ie ie8 lt-ie9" {{site.language_attributes}}> <![endif]-->
<!--[if gt IE 8]><!--><html class="no-js" {{site.language_attributes}}> <!--<![endif]-->
<head>
    <title>
        {% if wp_title %}
            {{ wp_title }} - {{ site.name }}
        {% else %}
            {{ site.name }}
        {% endif %}
    </title>

    <!-- Meta Tags  -->
    <meta charset="{{ site.charset }}" />
    <meta name="description" content="{{ site.description }}">
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!-- Social Share -->
    <meta property="og:locale" content="en_US"/>
    <meta property="og:type" content="article"/>
    <meta property="og:title" content="{{ post.title }}"/>
    <meta property="og:description" content="{{ site.description }}"/>
    <meta property="og:url" content="{{ post.link }}"/>
    <meta property="og:site_name" content="{{ site.name }}"/>
    <meta name="twitter:card" content="{{ site.name }}"/>
    <meta name="twitter:title" content="{{ post.title }}"/>
    <meta name="twitter:description" content="{{ site.description }}"/>
    <link rel="pingback" href="{{ site.pingback_url }}" />
    <!-- <meta property="og:image" content="{{ theme.link }}/assets/src/img/ "/> -->
    <!-- <meta name="twitter:image" content="{{ theme.link }}/assets/src/img/ "/> -->

    <!-- Links -->
    <link rel="stylesheet" href="https://unpkg.com/bootstrap@4.0.0/dist/css/bootstrap-grid.min.css">

    <!-- Scripts -->
    <script>
