{
    "name": "whiteboard/launchframe",
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org",
            "only": [
                "wpackagist-plugin/*",
                "wpackagist-theme/*"
            ]
        }
    ],
    "require": {
        "php": ">=7.4",
        "composer/installers": "^1.8",
        "oscarotero/env": "^2.1",
        "roots/bedrock-autoloader": "^1.0.4",
        "rareloop/lumberjack-core": "^4.3",
        "roots/soil": "^4.1.1",
        "roots/wordpress": "6.4.2",
        "roots/wp-config": "1.0.0",
        "roots/wp-password-bcrypt": "1.1.0",
        "timber/timber": "^1.18.2",
        "vlucas/phpdotenv": "^5.4",
        "wpackagist-plugin/acf-extended": "^0.8.9.5",
        "wpackagist-plugin/all-in-one-wp-migration": "^7.79",
        "wpackagist-plugin/autodescription": "^5.0.2",
        "wpackagist-plugin/duplicate-post": "^4.5",
        "wpackagist-plugin/minify-html-markup": "^2.1.9"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.7.1",
        "roave/security-advisories": "dev-latest"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "allow-plugins": {
            "composer/installers": true,
            "roots/wordpress-core-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "installer-paths": {
            "web/app/mu-plugins/{$name}/": [
                "type:wordpress-muplugin"
            ],
            "web/app/plugins/{$name}/": [
                "type:wordpress-plugin"
            ],
            "web/app/themes/{$name}/": [
                "type:wordpress-theme"
            ]
        },
        "wordpress-install-dir": "web/wp"
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "test": [
            "phpcs"
        ]
    }
}
