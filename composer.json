{
    "name": "whiteboard/launchframe",
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org",
            "only": [
                "wpackagist-plugin/*",
                "wpackagist-theme/*"
            ]
        },
        {
            "type": "composer",
            "url": "https://connect.advancedcustomfields.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "composer/installers": "^2.2",
        "oscarotero/env": "^2.1",
        "rareloop/lumberjack-core": "dev-master",
        "roots/bedrock-autoloader": "^1.0.4",
        "roots/soil": "^4.1.1",
        "roots/wordpress": "^6.6",
        "roots/wp-config": "1.0.0",
        "roots/wp-password-bcrypt": "1.1.0",
        "timber/timber": "2.3.0",
        "vlucas/phpdotenv": "^5.4",
        "wpackagist-plugin/acf-extended": "^0.9.1",
        "wpackagist-plugin/admin-menu-editor": "^1.13.1",
        "wpackagist-plugin/autodescription": "^5.1.2",
        "wpackagist-plugin/duplicate-post": "^4.5",
        "wpackagist-plugin/html-forms": "^1.5.3",
        "wpackagist-plugin/minify-html-markup": "^2.1.12",
        "wpackagist-plugin/safe-svg": "^2.3.1",
        "wpengine/advanced-custom-fields-pro": "^6.3"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.7.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "allow-plugins": {
            "composer/installers": true,
            "roots/wordpress-core-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "installer-paths": {
            "web/app/mu-plugins/{$name}/": [
                "type:wordpress-muplugin"
            ],
            "web/app/plugins/{$name}/": [
                "type:wordpress-plugin"
            ],
            "web/app/themes/{$name}/": [
                "type:wordpress-theme"
            ]
        },
        "wordpress-install-dir": "web/wp"
    },
    "scripts": {
        "test": [
            "phpcs"
        ]
    }
}
