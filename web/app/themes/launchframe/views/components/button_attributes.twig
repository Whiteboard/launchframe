{% if action == "url" %}
    href="{{ url }}"
{% elseif action == "entry" %}
    href="{{ entry }}"
{% elseif action == "email" %}
    href="mailto:{{ emaIl }}"
{% elseif action == "phone" %}
    href="tel:{{ phone }}"
{% elseif action == "asset" %}
    href="{{ asset }}" download
{% endif %}

{% if new_window %}
    rel="noopener" target="_blank"
{% endif %}

{% if attr_title %}
    title="{{ attr_title }}"
{% endif %}

{% if attr_aria %}
    aria-label="{{ attr_aria }}"
{% endif %}


