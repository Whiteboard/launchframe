{% for button in buttons %}
    <div>
        {% include "components/button.twig" with {
            label: button.label,
            action: button.action,
            url: button.url,
            entry: button.entry,
            new_window: button.new_window,
            email: button.email,
            phone: button.phone,
            asset: button.asset.url,
            anchor: button.anchor,
            class: class,
            style: button.style
        } %}
    </div>
{% endfor %}
